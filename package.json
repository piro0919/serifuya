{
  "babel": {
    "plugins": [
      [
        "react-css-modules",
        {
          "filetypes": {
            ".scss": {
              "syntax": "postcss-scss"
            }
          }
        }
      ]
    ],
    "presets": [
      "next/babel"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "1.2.0",
    "@react-hook/window-size": "3.0.7",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-sass": "1.0.1",
    "axios": "0.21.1",
    "babel-plugin-react-css-modules": "5.2.6",
    "dayjs": "1.9.7",
    "file-saver": "2.0.5",
    "generic-names": "2.0.1",
    "next": "10.0.3",
    "next-compose-plugins": "2.2.1",
    "next-localization": "0.10.0",
    "next-manifest": "3.0.1",
    "next-seo": "4.17.0",
    "node-sass": "4.14.1",
    "postcss-scss": "3.0.4",
    "rc-pagination": "3.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-h5-audio-player": "3.5.0",
    "react-hook-form": "6.13.1",
    "react-icons": "4.1.0",
    "react-no-ssr": "1.1.0",
    "react-share": "4.3.1",
    "react-switch": "5.0.1",
    "react-toastify": "6.2.0",
    "ress": "3.0.0",
    "sass-mq": "5.0.1",
    "uniqid": "5.2.0",
    "yup": "0.32.8"
  },
  "devDependencies": {
    "@types/file-saver": "2.0.1",
    "@types/node": "14.14.14",
    "@types/react": "16.14.2",
    "@types/react-css-modules": "4.6.2",
    "@types/react-no-ssr": "1.1.1",
    "@types/uniqid": "5.2.0",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.16.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "typescript": "4.0.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "aspects": [
            "invalidHref",
            "preferButton"
          ],
          "components": [
            "Link"
          ],
          "specialLink": [
            "hrefLeft",
            "hrefRight"
          ]
        }
      ]
    }
  },
  "name": "nextjs",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "eslint --fix --ext .ts,.tsx src/",
    "start": "next start"
  },
  "version": "0.1.0"
}
